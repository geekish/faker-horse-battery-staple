{
  "name": "geekish/faker-horse-battery-staple",
  "type": "library",
  "description": "Correct Horse Battery Staple password provider for Faker",
  "keywords": [
    "faker",
    "correct horse battery staple",
    "password"
  ],
  "homepage": "https://github.com/geekish/faker-horse-battery-staple",
  "license": "MIT",
  "authors": [
    {
      "name": "Hannah Warmbier",
      "email": "hannahwarmbier@gmail.com",
      "homepage": "https://github.com/geekish",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
    "fakerphp/faker": "^1.9",
    "yuloh/battery-staple": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5.20 || ^8.5.8 || ^9.4.2",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Geekish\\FakerHorseBatteryStaple\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Geekish\\FakerHorseBatteryStaple\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "checkcs": "./vendor/bin/phpcs",
    "fixcs": "./vendor/bin/phpcbf"
  },
  "config": {
    "sort-packages": true
  }
}
